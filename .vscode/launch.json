    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/bin/www",
            "env": {
              "NODE_ENV" : "development",
            },
            "console": "internalConsole"
        },
        {
          "type": "node",
          "request": "launch",
          "name": "Launch Current File",
          "skipFiles": [
              "<node_internals>/**"
          ],
          "program": "${file}",
          "env": {
            "NODE_ENV" : "development",
          },
          "console": "internalConsole"
        }      
    ]
}